package exerc;

public class Atividade_1_11 {
	public static void main(String[] args) {
		String S1 = args[0];
		String S2 = null;
		S2 = args[0] + args[1];
		
		int x = Integer.parseInt(S1);
		System.out.println(x);
		int y = Integer.parseInt(S1+S2);
		System.out.println(y);
		
		System.out.println(x+y);
	}
}
