public class numberone {
	public static void main(String[] args) {
		String S1 = args[0];
		String S2 = null;
		S2 = args[2] + args[0];
		String S3 = null;
		S3 = (args[1] + args[2]).concat(S1);
		int x = Integer.parseInt(S1);
		int y = Integer.parseInt(S1 + S2);
		int z = Integer.parseInt(S3);
		System.out.println(x + y + z);
	}
}
